.chose-list{
    overflow: hidden;
    margin-top: 60rpx;
}
.item{
    width: 100%;
    height: 228rpx;
    border-radius: 18rpx;
    box-sizing: border-box;
    padding: 40rpx 60rpx;
    position: relative;
    z-index: 1;
    margin-bottom: 30rpx;
    text-align: left;
    line-height:1.5;
}
.item-back{
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: -1;
}
.back-text{
    position: absolute;
    left: 40rpx;
    top: -40rpx;
    font-size: 153rpx;
    color: RGBA(255, 255, 255, 0.1);
    font-weight: 550;
}
.back-foot{
    position: absolute;
    color: RGBA(255, 255, 255, 0.3);
    font-size: 24rpx;
    bottom: 20rpx;
    right: 20rpx;
}
.back-brand{
    margin-right: 40rpx;
}
.name{
    color: #fff;
}
.kol{
    background:  linear-gradient(to bottom,RGBA(255, 71, 139, 1),RGBA(224, 0, 134, 1));
}
.brand{
    background:  linear-gradient(to bottom,RGBA(143, 48, 252, 1),RGBA(58, 0, 255, 1));
}
.mcn{
    background:  linear-gradient(to bottom,RGBA(54, 208, 117, 1),RGBA(11, 170, 165, 1));
}
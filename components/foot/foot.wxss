.nav-foot {
    height: 104rpx;
    line-height: 104rpx;
    position: fixed;
    bottom: 0;
    width: 100%;
    left: 0;
    display: flex;
    flex-direction: row;
    align-items: center;
    box-sizing: border-box;
    /* border-top: 1rpx solid #333; */
    font-size:16px;
    background: #fff;
}

.nav-foot .item {
    text-align: center;
    flex-grow: 1;
    color:rgba(40,40,40,.3);
}

.active {
    color: rgb(40,40,40);
}

.nav-foot .item image {
    width: 94rpx;
    height: 30rpx;
}

.nav-foot .item .nav {
    display: inline-block;
    width: 100%;
    height: 100%;
}